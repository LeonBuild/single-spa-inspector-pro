{
  "name": "single-spa-inspector-pro-mcp",
  "version": "0.0.1",
  "type": "module",
  "description": "MCP server for single-spa Inspector Pro - AI bridge to real Chrome tabs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sspa-mcp": "./bin.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w",
    "typecheck": "tsc --noEmit",
    "start:relay": "tsx src/relay.ts",
    "start:mcp": "tsx src/mcp.ts"
  },
  "keywords": [
    "mcp",
    "single-spa",
    "chrome-extension",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@hono/node-ws": "^1.2.0",
    "@modelcontextprotocol/sdk": "^1.21.1",
    "chalk": "^5.6.2",
    "devtools-protocol": "^0.0.1543509",
    "hono": "^4.10.6",
    "playwright-core": "^1.56.1",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/ws": "^8.18.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
